{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "member-ordering": [
            true,
            {
                "order": "fields-first"
            }
        ],
        "no-console": {
            "severity": "warning",
            "options": [
                "log"
            ]
        },
        "object-literal-sort-keys": false,
        "jsx-no-lambda": false,
        "jsx-no-multiline-js": false
    }
}
